  "scripts": {
    "build-prod": "ng build --prod",
    "build": "ng build",
    "clean": "rm -r dist node_modules yarn.lock package-lock.json",
    "lint-fix": "ng lint coding-experiment --fix && ng lint coding-experiment-e2e --fix",
    "lint-scss-fix": "stylelint \"src/**/*.scss\" --fix",
    "lint-scss": "stylelint \"src/**/*.scss\"",
    "lint": "ng lint coding-experiment --format stylish",
    "prettier": "prettier --config ./.prettierrc.json --write \"{src,e2e,functions}/**/*.{ts,js,json,scss}\"",
    "serve": "ng serve -o",
    "update": "yarn upgrade-interactive --latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/cdk": "^6.3.3",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/material": "^6.3.3",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "@angular/service-worker": "^6.0.9",
    "bootstrap": "^4.1.2",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "rxjs": "6.2.2",
    "zone.js": "^0.8.26"
